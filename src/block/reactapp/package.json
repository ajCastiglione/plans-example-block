{
	"name": "react-js-mini-boilerplate",
	"version": "1.0.0",
	"description": "Simple boilerplate for bootstrapping react w/ parcel.",
	"main": "index.js",
	"scripts": {
		"start": "rm -rf ./.cache && parcel src/index.js --out-dir ../../../dist -o react.build.js",
		"build": "rm ../../../react.build.* && parcel build src/index.js --out-dir ../../../dist -o react.build.js"
	},
	"keywords": [],
	"author": "Antonio Castiglione",
	"devDependencies": {
		"@babel/core": "^7.9.0",
		"@babel/plugin-transform-runtime": "^7.9.0",
		"@babel/runtime-corejs2": "^7.9.2",
		"parcel-bundler": "^1.12.4",
		"sass": "^1.26.3"
	},
	"dependencies": {
		"node-sass": "^6.0.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2"
	}
}
